<section id="page-title" class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12 text-center">
        <h3>Browse Videos</h3>
      </div>
    </div>
  </div>
</section>
<div class="col-md-2"></div>
<div class="col-md-8">
  <ul class="video-tiles">
    <% @videos.each do |video| %>
      <li>
        <%= link_to video_path(video) do %>
          <%= image_tag(video.thumbnail, :class => "vid-thumbnail") %>
          <br>
          <!-- <td><%= link_to "#{video.title}", video %></td> -->
        <% end %>
      </li>
    <% end %>
  </ul>
</div>
<div class="col-md-2"></div>